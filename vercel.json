{
    "version": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "HEAD",
                "DELETE",
                "OPTIONS"
            ]
        }
    ],
    "crons": [
        {
            "path": "/api/queue/cron",
            "schedule": "*/5 * * * *"
        }
    ]
}